<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>anilla Races Expanded - Android</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				
				<li Class="PatchOperationAdd">
					<xpath>/Defs/VREAndroids.AndroidGeneDef[defName="VREA_SyntheticImmunity"]</xpath>
					<value>
						<makeImmuneTo><li>VRE_GeneInfection</li></makeImmuneTo>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>
</Patch>